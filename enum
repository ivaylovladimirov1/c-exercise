using System;

namespace enums
{
    public enum ShippingMethod
    {
        RegularMail=1,
        AirMail=2,
        Express=3

    }
    class Program
    {
        static void Main(string[] args)
        {
            var method = ShippingMethod.Express;
            Console.WriteLine((int)(method));

            var methodId = 3;
            Console.WriteLine((ShippingMethod),methodId);
        }
    }
}
